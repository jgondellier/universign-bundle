{
    "name": "gondellier/universign-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "Minimal project to use universign services.",
    "authors": [
        {
            "name": "Jonathan Gondellier",
            "email": "jgondellier@yahoo.fr"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-xmlrpc": "*",
        "guzzlehttp/guzzle": "~6.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
        },
        "post-install-cmd": [
        ],
        "post-update-cmd": [
        ]
    },
    "conflict": {
    },
    "extra": {
    }
}
